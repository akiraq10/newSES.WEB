<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Regression Test" parallel="methods" thread-count="4">
    <listeners>
        <listener class-name="com.clean.listener.ListenerEx"></listener>
        <listener class-name="com.clean.listener.RetestFailedCase"></listener>
    </listeners>
    <test name="Basic test ses.WEB">
        <groups>
            <run>
                <include name="regression">
                </include>
            </run>
        </groups>
        <classes>
            <class name="testclass.server.LoginTest"></class>
            <class name="testclass.client.sesweb.UsersTest"/>
            <class name="testclass.client.sesweb.grouptest.GroupTest"></class>
            <class name="testclass.client.sesweb.ProfileTest"></class>
            <class name="testclass.client.sesweb.PackageTest"></class>
            <class name="testclass.client.sesweb.AdministratorTest"></class>
            <class name="testclass.client.sesweb.FolderTest"></class>
            <class name="testclass.client.sesweb.grouptest.ActionFromUserMenu"></class>
            <class name="testclass.client.sesweb.grouptest.ActionFromUserAdvancedMenu"></class>
        </classes>
    </test>


</suite>