<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite thread-count="4" parallel="tests" name="Regression">
    <listeners>
        <listener class-name="com.clean.listener.ListenerEx"/>
        <listener class-name="com.clean.listener.RetestFailedCase"/>
    </listeners>
    <test thread-count="4" parallel="tests" name="chrome">
        <parameter name="browser" value="firefox"/>
        <parameter name="uri" value="https://messo.asia"/>
        <groups>
            <run>
                <include name="regression"/>
            </run>
        </groups>
        <classes>
            <class name="testclass.client.LoginTest"/>
            <class name="testclass.client.sesweb.AdministratorTest"/>
            <class name="testclass.client.sesweb.FolderTest"/>
            <class name="testclass.client.sesweb.grouptest.ActionFromGroupMenu"/>
            <class name="testclass.client.sesweb.grouptest.ActionFromUserAdvancedMenu"/>
            <class name="testclass.client.sesweb.grouptest.ActionFromUserMenu"/>
            <class name="testclass.client.sesweb.grouptest.GroupTest"/>
            <class name="testclass.client.sesweb.PackageTest"/>
            <class name="testclass.client.sesweb.ProfileTest"/>
            <class name="testclass.client.sesweb.user.viewproperties.GeneralTest"/>
            <class name="testclass.client.sesweb.UsersTest"/>
        </classes>
    </test> <!-- chrome -->
</suite> <!-- Regression -->